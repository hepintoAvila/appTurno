{
  "compilerOptions": {
    "typeRoots": ["src/@types", "node_modules/@types"],
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "lib": ["dom", "es2015"],
      "moduleResolution": "node"
    },
    "include": ["src"]
  },

}
